{
    "name": "berlinonline/dat0r",
    "type": "library",
    "bin": ["bin/dat0r.make", "bin/dat0r.console"],
    "description": "Library for generating domain/data objects.",
    "keywords": ["entity", "scaffolding"],
    "homepage": "https://github.com/berlinonline/Dat0r",
    "license": "MIT",
    "authors": [
        {
            "name": "Thorsten Schmitt-Rink"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5.0",
        "beberlei/assert": "2.3",
        "egulias/email-validator": "1.2.*",
        "fzaninotto/faker": "1.4.*",
        "mtdowling/jmespath.php": "2.1.*",
        "rhumsaa/uuid": "2.8.*",
        "symfony/console": "2.6.*",
        "symfony/filesystem": "2.6.*",
        "twig/twig": "1.18.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*",
        "phpmd/phpmd": "2.2.*",
        "sebastian/phpcpd": "2.0.*",
        "squizlabs/php_codesniffer": "2.2.*",
        "mayflower/php-codebrowser": "1.1.*",
        "mockery/mockery": "0.9.*",
        "satooshi/php-coveralls": "0.6.*",
        "sami/sami": "2.0.*"
    },
    "autoload": {
        "psr-4": { "Dat0r\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Dat0r\\Tests\\" : "tests/" }
    }
}
